				<thrust_coeff  forward="0.167" reverse="0.167"/>
				<torque_coeff value="0.016"/>
			</thrust_model>
			<propeller diameter="0.076" right="true">
				<mesh filename="bluerov2/ccw.obj" scale="1.0"/>
